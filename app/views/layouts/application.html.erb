<!DOCTYPE html>
<html>
<head>
  <title>AvtomatKalashnikova</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
	<section id="wrapper" class="container">
		<%= yield %>
	</section>

  <% if user_signed_in? %>
    <%= render 'deposits/send_coins_modal' %>
    <%= render 'reports/report_modal' %>
    <%= render 'items/import_card_modal' %>
  <% end %>
</body>
</html>
