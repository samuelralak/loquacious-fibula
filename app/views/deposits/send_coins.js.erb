console.log("COINS: <%= @coins %>");
console.log("ADDRESS: <%= @address %>");
console.log("RESPONSE: <%= @response %>");
if (<%= @error %>) {
  $('#myModal').modal('hide')
  sweetAlert("Oops...", "<%= @exception %>!", "error");
} else {
  $('#myModal').modal('hide')
  sweetAlert("Success!", "Request successfully sent", "success");
}
