console.log("COINS: <%= @coins %>");
console.log("ADDRESS: <%= @address %>");
console.log("RESPONSE: <%= @response %>");
if (<%= @error %>) {
  $('#myModal').modal('hide')
  sweetAlert("Oops...", "<%= @exception %>!", "error");
}

if (<%= @success %>) {
  $('#myModal').modal('hide')
  sweetAlert("Success!", "You have successfully transfered <%= @coins %> coins to <%= @address %>", "success");
}
